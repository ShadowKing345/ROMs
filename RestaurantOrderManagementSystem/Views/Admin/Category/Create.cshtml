@model RestaurantOrderManagementSystem.Controllers.CategoryViewModel

<div>
    <h2>Create New Category</h2>
    <form asp-controller="MenuItem" asp-action="CreateMenuItemCategory" method="post" enctype="multipart/form-data">
        <fieldset>
            <partial name="Category/_CategoryForm" model="@Model"/>
        </fieldset>
        <br/>
        <div class="Div">
            <a class="LinkButton Button OkayButton" href="javascript:$('form').submit()">Create Category</a>
            <a class="LinkButton Button ErrorButton" asp-controller="Admin" asp-action="CategoryDashBoard">Cancel</a>
        </div>
    </form>
</div>
